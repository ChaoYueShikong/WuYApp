<view id="home-list"><cu-custom vue-id="1" bgColor="bg-gradual-white" displayable isBack="{{false}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">门禁设备</view></cu-custom><view class="cu-bar bg-white search fixed" style="{{'top:'+(CustomBar+'px')+';'}}"><view class="search-form round"><text class="cuIcon-search"></text><input type="text" placeholder="搜索幢数如: 6幢" data-event-opts="{{[['input',[['__set_model',['','inputResult','$event',[]]]]]]}}" value="{{inputResult}}" bindinput="__e"/></view><view class="action"><button data-event-opts="{{[['tap',[['searchBuilding']]]]}}" class="cu-btn bg-gradual-blue shadow-blur round" bindtap="__e">搜索</button></view></view><view class="VerticalBox"><scroll-view class="VerticalNav nav" style="height:calc(100vh - 64px - 50px);" scroll-y scroll-with-animation scroll-top="{{verticalNavTop}}"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item',index==tabCur?'text-blue cur ':'']}}" data-id="{{index}}" data-event-opts="{{[['tap',[['TabSelect',['$event']]]]]}}" bindtap="__e">{{''+item.name+''}}</view></block></scroll-view><scroll-view class="VerticalMain" style="height:calc(100vh - 64px - 50px);padding-left:4%;padding-right:4%;" scroll-y scroll-with-animation scroll-into-view="{{'main-'+mainCur}}" data-event-opts="{{[['scroll',[['VerticalMain',['$event']]]]]}}" bindscroll="__e"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="padding-lr" style="background:white;border:white;border-radius:20rpx;margin-top:4%;" id="{{'main-'+index}}"><view class="cu-bar solid-bottom bg-white"><view class="action"><text class="cuIcon-title text-blue"></text>{{item.name+''}}</view></view><block wx:for="{{item.units}}" wx:for-item="item2" wx:for-index="index2" wx:key="index2"><view class="cu-list menu-avatar" id="{{'item-'+index2}}"><view class="cu-item"><view class="cu-avatar" style="background-image:url(/static/img/ic_house_black.png);"></view><view data-event-opts="{{[['tap',[['toDeviceDetail',['$0','$1'],[[['listData','',index,'buildingsId']],[['listData','',index],['units','',index2,'unitsId']]]]]]]}}" class="content" bindtap="__e"><view class="text-black">{{item2.uName}}</view></view><view data-event-opts="{{[['tap',[['openLock',['$0'],[[['listData','',index],['units','',index2]]]]]]]}}" class="action" bindtap="__e"><button class="cu-tag round  shadow  text-white" style="{{'background-color:'+(item2.status=='离线中···'?'#8799a3':'#0081ff')+';'}}">{{''+item2.status+''}}</button></view></view></view></block></view></block></scroll-view></view></view>